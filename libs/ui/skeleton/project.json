{
  "name": "ui-skeleton",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ui/skeleton/src",
  "prefix": "ui",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:ng-packagr",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/ui/skeleton/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui/skeleton/tsconfig.lib.json"
        },
        "development": {
          "tsConfig": "libs/ui/skeleton/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ui/skeleton/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "tags": ["type:ui"]
}

