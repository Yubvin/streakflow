{
  "name": "ui-avatar",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/avatar/src",
  "prefix": "ui",
  "tags": ["ui", "avatar"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/ui/avatar"],
      "options": {
        "project": "libs/ui/avatar/ng-package.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/ui/avatar"],
      "options": {
        "jestConfig": "libs/ui/avatar/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ui/avatar/**/*.ts", "libs/ui/avatar/**/*.html"]
      }
    }
  }
}
